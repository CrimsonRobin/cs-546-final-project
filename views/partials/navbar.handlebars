<header>
    <a href="/">
        <img src="/public/assets/logo.svg" alt="a11y logo">
    </a>
    <div>
        {{#if user}}
            <a href="/user?id={{user.id}}" >
            <span class="material-symbols-outlined" aria-label="Account">account_circle</span>
            </a>
        {{else}}
            <a href="/login" class="button-link">Log In</a>
            <a href="/register" class="button-link">Register</a>
        {{/if}}
    </div>
</header>